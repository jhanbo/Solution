<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SystemSetting" table="system_setting" schema="itg_wd" lazy="false">
    
    <id name="SystemSettingId" type="int">
      <column name="idsystem_setting" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    
    <property name="Name" type="String">
      <column name="name" not-null="true" length="100" sql-type="varchar" />
    </property>
    
    <property name="Value" type="String">
      <column name="value" not-null="false" length="255" sql-type="varchar" />
    </property>
    
  </class>
</hibernate-mapping>