<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Assignment" table="assignment" schema="itg_wd" lazy="false">
    
    <id name="AssignmenId" type="int">
      <column name="idassignment" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    
    <property name="AssignedDate" type="DateTime">
      <column name="assigned_date" not-null="false" sql-type="datetime" />
    </property>
    
    <property name="ReceivedDate" type="DateTime">
      <column name="received_date" not-null="false" sql-type="datetime" />
    </property>
    
    <property name="DueDate" type="DateTime">
      <column name="due_date" not-null="false" sql-type="datetime" />
    </property>
    
    <property name="RemindedDate" type="DateTime">
      <column name="reminded_date" not-null="false" sql-type="datetime" />
    </property>
    
    <property name="Actions" type="String">
      <column name="actions" not-null="false" length="255" sql-type="varchar" />
    </property>
   
    <property name="State" type="int">
      <column name="state" not-null="false" precision="10" scale="0" sql-type="int" />
    </property>
    
    <many-to-one name="Correspondence" class="Correspondence">
      <column name="correspondence_idcorrespondence" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
    
    <many-to-one name="User" class="User">
      <column name="itg_user_iditg_user" not-null="false" precision="10" scale="0" sql-type="int" />
    </many-to-one>
    
    <many-to-one name="Orgunit" class="OrganizationUnit">
      <column name="orgunit_idorgunit" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
    
    <bag name="AssignmentAttachments" inverse="true" lazy="false">
      <key>
        <column name="assignment_idassignment" />
      </key>
      <one-to-many class="AssignmentAttachment" />
    </bag>
    
  </class>
</hibernate-mapping>