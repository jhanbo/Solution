<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
	<class name="CorrespondenceNote" table="corr_notes" schema="itg_wd" lazy="false">
		<id name="ID" type="int">
			<column name="idcorr_notes" not-null="true" precision="10" scale="0" sql-type="int" />
			<generator class="increment" />
		</id>

		<property name="Note" type="String">
			<column name="corr_note" not-null="true" length="500" sql-type="varchar" />
		</property>

		<property name="NoteDate" type="DateTime">
			<column name="note_date" not-null="true" length="500" sql-type="datetime" />
		</property>

		<many-to-one name="Correspondence" class="Correspondence">
			<column name="correspondence_idcorrespondence" not-null="true" precision="10" scale="0" sql-type="int" />
		</many-to-one>

		<many-to-one name="User" class="User">
			<column name="itg_user_iditg_user" not-null="true" precision="10" scale="0" sql-type="int" />
		</many-to-one>
		
	</class>
</hibernate-mapping>