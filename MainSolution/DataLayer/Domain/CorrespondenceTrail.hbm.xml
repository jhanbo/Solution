<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CorrespondenceTrail" table="corr_trails" schema="itg_wd" lazy="false">
    <id name="CorrespondenceTrailsId" type="int">
      <column name="idcorr_trails" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    
    <property name="Description" type="String">
      <column name="description" not-null="false" length="255" sql-type="varchar" />
    </property>
    
    <property name="ActionDate" type="DateTime">
      <column name="action_date" not-null="false" sql-type="datetime" />
    </property>
    
    <many-to-one name="User" class="User">
      <column name="itg_user_iditg_user" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
    
    <many-to-one name="Correspondence" class="Correspondence">
      <column name="correspondence_idcorrespondence" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
    
  </class>
</hibernate-mapping>