<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
                   assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain">

  <class name="UserRolesOrg" lazy="false" table="user_role_org">
    <composite-id>
      <key-property name="Role" type="int">
        <column name="role_idrole" not-null="true" precision="10" scale="0" sql-type="int" />
      </key-property>
      <key-property name="Orgunit" type="int">
        <column name="orgunit_idorgunit" not-null="true" precision="10" scale="0" sql-type="int" />
      </key-property>
      <key-property name="User" type="int">
        <column name="itg_user_iditg_user" not-null="true" precision="10" scale="0" sql-type="int" />
      </key-property>
    </composite-id>
    
    
      <many-to-one name="Role" class="Role">
        <column name="role_idrole" not-null="true" sql-type="int" />
      </many-to-one>

      <many-to-one name="Orgunit" class="OrganizationUnit">
        <column name="orgunit_idorgunit" not-null="true" sql-type="int" />
      </many-to-one>

      <many-to-one name="User" class="User">
        <column name="itg_user_iditg_user" not-null="true" sql-type="int" />
      </many-to-one>

  </class>

</hibernate-mapping>