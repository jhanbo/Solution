<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CorrespondenceAttachment" table="corr_attachment" schema="itg_wd" lazy="false">
    
    <id name="CorrespondenceAttachmentId" type="int">
      <column name="idcorr_attachment" not-null="true" precision="10" scale="0" sql-type="int" />
      <generator class="identity" />
    </id>
    
    <property name="Name" type="string">
      <column name="name" not-null="false" precision="10" scale="0" sql-type="varchar" />
    </property>
    
    <property name="Description" type="string">
      <column name="description" not-null="false" precision="10" scale="0" sql-type="varchar" />
    </property>
    
    <property name="CreatedDate" type="DateTime">
      <column name="created_date" not-null="false" precision="10" scale="0" sql-type="datetime" />
    </property>
    
    <property name="DataFile" type="BinaryBlob">
      <column name="data_file" not-null="false" length="65535" sql-type="blob" />
    </property>
    
    <many-to-one name="Correspondence" class="Correspondence">
      <column name="correspondence_idcorrespondence" not-null="true" precision="10" scale="0" sql-type="int" />
    </many-to-one>
  </class>
</hibernate-mapping>