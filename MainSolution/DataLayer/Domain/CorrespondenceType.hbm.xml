<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITGateWorkDesk.Data" namespace="ITGateWorkDesk.Data.Domain" xmlns="urn:nhibernate-mapping-2.2">
	<class name="CorrespondenceType" table="corr_types" lazy="false" >
		<id name="ID">
			<column name="idcorr_types" sql-type="int(11)" not-null="true" />
			<generator class="identity" />
		</id>
		<property name="CorrTypeCode">
			<column name="corr_Type_code" sql-type="varchar(10)" not-null="true" />
		</property>
		<property name="CorrTypeDesc">
			<column name="corr_type_desc" sql-type="varchar(100)" not-null="true" />
		</property>
	</class>
</hibernate-mapping>