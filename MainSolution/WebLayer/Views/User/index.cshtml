@model System.Web.Security.MembershipUserCollection

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>
<p>
    @Html.ActionLink("Create New User", "Create")
</p>
<table>
<tr>
    <th>User Name</th>
    <th>Email</th>

  
 </tr>

 @foreach (System.Web.Security.MembershipUser user in this.Model)
 {  
     <tr>
     <td>
     @Html.DisplayFor(modelItem => user.UserName)
     </td>
     <td>
      @Html.DisplayFor(modelItem => user.Email)
     </td>
     <td>
            @Html.ActionLink("Edit", "Edit", new { id = user.ProviderUserKey }) |
            @Html.ActionLink("Delete", "Delete", new { id = user.ProviderUserKey }, new { onclick = "return confirm('Are you sure you want to delete " + user.ProviderUserKey+ " User ?')" })
        </td>
     </tr>
 }
</table>
